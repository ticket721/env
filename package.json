{
    "name": "@ticket721/env",
    "private": true,
    "version": "1.0.0",
    "description": "env - Complete Environment for ticket721",
    "main": "index.js",
    "scripts": {
        "t721:init": "git submodule foreach npm install && gulp init",
        "t721:fetch": "git submodule update --recursive --remote --init",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ticket721/env.git"
    },
    "author": "Iulian Rotaru",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ticket721/env/issues"
    },
    "homepage": "https://github.com/ticket721/env#readme",
    "dependencies": {
        "@htmlacademy/editorconfig-cli": "^1.0.0",
        "axios": "^0.19.0",
        "ethereumjs-tx": "^1.3.7",
        "form-data": "^2.3.3",
        "gulp": "^4.0.2",
        "gulp-shell": "^0.6.5",
        "husky": "^1.3.1",
        "portalize": "^1.0.4",
        "signale": "^1.4.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^7.5.2",
        "@commitlint/config-conventional": "^7.5.0",
        "conventional-changelog-cli": "^2.0.12",
        "deep-extend": ">=0.5.1",
        "merge": ">=1.2.1"
    },
    "editorconfig-cli": [
        "./gulpfile.js",
        "./package.json"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "editorconfig-cli",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
