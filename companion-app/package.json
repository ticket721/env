{
    "name": "@ticket721/companion-app",
    "private": true,
    "version": "0.1.0",
    "description": "companion-app - Mobile companion app for offline ticket access",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "editorconfig-cli": "editorconfig-cli",
        "test:editorconfig": "npm run editorconfig-cli",
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "test:mocha": "env MOCHA_TESTING=true nyc ts-mocha -p tsconfig.json ./sources/**/*.test.ts ./sources/**/**/*.test.ts",
        "test:lint": "tslint --project ."
    },
    "nyc": {
        "extends": "@istanbuljs/nyc-config-typescript",
        "all": true,
        "reporter": [
            "text",
            "lcov"
        ],
        "include": [
            "sources/core/**.ts",
            "sources/core/**/**.ts"
        ],
        "exclude": [
            "sources/core/CompanionIdentifierRetriever/CompanionIdentifierRetrieverExpo.ts",
            "sources/core/log.ts",
            "sources/core/**.test.ts",
            "sources/core/**/**.test.ts"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ticket721/companion-app.git"
    },
    "author": "Iulian Rotaru",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ticket721/companion-app/issues"
    },
    "homepage": "https://github.com/ticket721/companion-app#readme",
    "dependencies": {
        "@ant-design/react-native": "^3.1.13",
        "@eva-design/eva": "^1.0.1",
        "@htmlacademy/editorconfig-cli": "^1.0.0",
        "@redux-saga/types": "^1.0.2",
        "@ticket721/e712": "0.1.1",
        "bignumber.js": "^9.0.0",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "ethereumjs-wallet": "^0.6.3",
        "ethers": "^4.0.33",
        "expo": "^34.0.1",
        "expo-brightness": "^6.0.0",
        "expo-font": "^6.0.1",
        "expo-localization": "^6.0.0",
        "fuse.js": "^3.4.5",
        "geopattern": "^1.2.3",
        "i18n-js": "^3.3.0",
        "js-logger": "^1.6.0",
        "mocha": "^6.2.0",
        "moment": "^2.24.0",
        "portalize": "^1.0.4",
        "pouchdb-adapter-asyncstorage": "^6.4.1",
        "qrious": "^4.0.2",
        "react": "16.8.3",
        "react-dom": "^16.8.6",
        "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.0.tar.gz",
        "react-native-gesture-handler": "^1.3.0",
        "react-native-qrcode-svg": "^5.2.0",
        "react-native-svg": "^9.5.1",
        "react-native-ui-kitten": "^4.2.0-beta.1",
        "react-native-web": "^0.11.7",
        "react-navigation": "^3.12.0",
        "react-redux": "^7.1.0",
        "redux": "^4.0.4",
        "redux-saga": "^1.0.5",
        "rxdb": "^8.3.0",
        "rxjs": "^6.5.2",
        "strapi-sdk-javascript": "^0.3.1",
        "url-parse": "^1.4.7"
    },
    "editorconfig-cli": [
        "./package.json"
    ],
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^0.1.3",
        "@types/expect": "^1.20.4",
        "@types/jest": "^24.0.18",
        "@types/mocha": "^5.2.7",
        "@types/react": "^16.8.23",
        "@types/react-native": "^0.57.65",
        "babel-preset-expo": "^6.0.0",
        "deep-extend": ">=0.5.1",
        "eth-sig-util": "^2.4.0",
        "jest": "^24.9.0",
        "linted": "^1.0.5",
        "merge": ">=1.2.1",
        "nyc": "^14.1.1",
        "pouchdb-adapter-memory": "^7.1.1",
        "sinon": "^7.4.1",
        "source-map-support": "^0.5.13",
        "ts-jest": "^24.0.2",
        "ts-mocha": "^6.0.0",
        "tslint": "^5.19.0",
        "tslint-react": "^4.0.0",
        "typescript": "^3.5.3"
    }
}
